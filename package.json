{
  "name": "@purecore/one-configurator-4-all",
  "version": "0.1.0",
  "description": "> **One Configurator 4 All** - Uma biblioteca ultra-leve e independente para gerenciamento de configurações em Node.js/JavaScript",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "test": "npm run test:purecore",
    "test:purecore": "node --test test/purecore.test.js",
    "test:jest": "node_modules/.bin/jest test/jest.test.js --no-cache",
    "test:vitest": "node_modules/.bin/vitest run test/vitest.test.js",
    "test:compare": "node compare-testers.js",
    "example:server": "node examples/server-config.js",
    "example:db": "node examples/database-config.js",
    "example:global": "node examples/test-global-config.js",
    "examples": "npm run example:global && npm run example:db && npm run example:server"
  },
  "keywords": [
    "config",
    "one-configurator-4-all",
    "configuração",
    "configurações",
    "configuração de aplicações",
    "configuração de aplicações em Node.js",
    "configuração de aplicações em JavaScript",
    "configuração de aplicações em TypeScript"
  ],
  "author": "suissa",
  "repository": {
    "type": "git",
    "url": "https://github.com/suissa/purecore-one-configurator-4-all"
  },
  "bugs": {
    "url": "https://github.com/suissa/purecore-one-configurator-4-all/issues"
  },
  "homepage": "https://github.com/suissa/purecore-one-configurator-4-all",
  "license": "MIT",
  "devDependencies": {
    "jest": "^30.2.0",
    "vitest": "^4.0.16"
  }
}
